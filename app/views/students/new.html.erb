<h1>Student Form</h1>
<%= form_tag students_path do %>
  <%= label_tag :student_first_name, "First Name:"%> <br>
  <%= text_field_tag :student_first_name %> <br>
  <br>
  <%= label_tag :student_last_name, "Last Name: "%> <br>
  <%= text_field_tag :student_last_name %> <br>
  <br>
  <%= submit_tag "Create Student" %>

<% end %>
