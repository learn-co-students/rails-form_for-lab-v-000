<h1>Student Form</h1>
<p>Find me in app/views/students/new.html.erb</p>

<%= form_tag students_path do %>
  <%= label_tag 'student[first_name]', 'First Name: ' %>
  <%= text_field_tag 'student[first_name]' %><br><br>

  <%= label_tag 'student[last_name]', 'Last Name: ' %>
  <%= text_field_tag 'student[last_name]' %><br><br>

  <%= submit_tag 'Create Student' %>
<% end %>
