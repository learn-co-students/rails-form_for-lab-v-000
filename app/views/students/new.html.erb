<h1>Student Form</h1>
<%= form_tag students_path do %>

  <label>First Name:</label><br>
  <%= text_field_tag "student[first_name]", nil, id: 'student_first_name' %><br>

  <label>Last Name:</label><br>
  <%= text_field_tag "student[last_name]", nil, id: 'student_last_name' %><br>

  <%= submit_tag "Create Student" %>
<% end %>
