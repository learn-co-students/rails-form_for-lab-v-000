<h1>New Student Form</h1>
<%= form_tag students_path do  %>
  <%= label_tag :student_first_name, "First Name" %>
  <%= text_field_tag :student_first_name %> <br/>

  <%= label_tag :student_last_name, "Last Name" %>
  <%= text_field_tag :student_last_name %>

  <%= submit_tag  "Create Student" %>
<% end %>
