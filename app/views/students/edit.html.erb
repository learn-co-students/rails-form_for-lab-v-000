<h1>Students#edit</h1>
<p>Find me in app/views/students/edit.html.erb</p>

<%= form_for @student do |f| %>
  <p>
   <%= f.label :first_name %><br>
   <%= f.text_field :first_name %>
  </p>
  <p>
   <%= f.label :last_name %><br>
   <%= f.text_field :last_name %>
  </p>
  <p>
   <%= f.submit %>
  </p>
  <% end %>