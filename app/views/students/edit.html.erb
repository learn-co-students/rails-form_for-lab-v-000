<%= form_tag students_path do %>
  <h1>Student Form</h1>
  <label>Fist Name:</label><br>
  <input type="text" id="student_first_name" name="student[first_name]" value=<%= @student.first_name %>><br>
  <!--<%= text_field_tag :'first_name' %><br>-->

  <label>Last Name:</label><br>
  <!--<%= text_field_tag :'last_name' %><br>-->
  <input type="text" id="student_last_name" name="student[last_name]" value=<%= @student.first_name %><br>

  <%= submit_tag "Update Student" %>
<% end %>
