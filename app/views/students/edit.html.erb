<h1>Edit <%=@student.to_s%> </h1>

<%= form_for(@student) do |f| %>

  <%=f.label :first_name %>:
  <%=f.text_field :first_name, :value => @student.first_name%><br>

  <%=f.label :last_name %>:
  <%= f.text_field :last_name, :value => @student.last_name %><br>

  <%= f.submit "Update Student" %>

  <%end%>
