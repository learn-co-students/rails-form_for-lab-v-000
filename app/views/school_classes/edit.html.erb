<h1>SchoolClasses#edit</h1>
<p>Find me in app/views/school_classes/edit.html.erb</p>

<%= form_for(@school_class) do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title, value: @school_class.title %>
  <%= f.label :room_number %>
  <%= f.text_field :room_number, value: @school_class.room_number %>
  <%= f.submit %>
<% end %>
