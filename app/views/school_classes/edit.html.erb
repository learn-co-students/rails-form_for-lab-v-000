<h1>School Class Form!</h1>
<%= form_for :school_class, url:school_classes_path do |f| %>
  <%= f.label :title %>:
  <%= f.text_field :title, value: @school_class.title %><br>
  <%= f.label :room_number %>:
  <%= f.text_field :room_number , value: @school_class.room_number %><br>

  <%= f.submit "Update School class" %>
<% end %>
