<h1>School Class Form:</h1>

<%= form_for(@school_class) do |f| %>
  <label>Class Title:</label><br>
  <%= f.text_field :title, placeholder: "Edit Title" %><br>

  <label>Class Room Number:</label><br>
  <%= f.number_field :room_number, placeholder: "Edit Room Number" %><br>

  <%= f.submit %>
<% end %>