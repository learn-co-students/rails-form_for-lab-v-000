<h1>School Class Form</h1>
<%= form_for(:school_class, url: school_classes_path) do |f| %>
  <%= f.label :title, "Class Name: " %>
  <%= f.text_field :title %><br></br>
  <%= f.label :room_number, "Room Number: " %>
  <%= f.text_field :room_number %><br></br>
  <%= f.submit 'Create School class'%>
<%end%>
