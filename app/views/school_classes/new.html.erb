<h1>School Class Form</h1>
<%= form_tag school_classes_path do %>

  <label>Title:</label><br>
  <%= text_field_tag "school_class[title]", nil, id: 'school_class_title' %><br>

  <label>Room Number:</label><br>
  <%= text_field_tag "school_class[room_number]", nil, id: 'school_class_room_number'  %><br>

  <%= submit_tag "Create School class" %>
<% end %>
